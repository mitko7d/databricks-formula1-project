   "source": [
    "select driver_name,\n",
    "      count(*) total_races,\n",
    "      sum(calculated_points) total_points,\n",
    "      avg(calculated_points) avg_points\n",
    "from f1_presentation.calculated_race_results\n",
    "where race_year between 2010 and 2020\n",
    "group by driver_name\n",
    "having count(*) > 50\n",
    "order by avg_points desc"
